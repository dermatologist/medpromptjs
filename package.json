{
  "name": "medpromptjs",
  "version": "0.2.3",
  "description": "A library of langchain base classes for building medical applications.",
  "license": "Apache-2.0",
  "author": "Bell Eapen",
  "main": "dist/index.js",
  "module": "dist/medpromptjs.esm.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/medpromptjs.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/medpromptjs.esm.js": {
      "import": "./dist/medpromptjs.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --verbose --testPathIgnorePatterns=./test/e2e/*",
    "e2e": "dts test --verbose ./test/e2e --detectOpenHandles",
    "docs": "typedoc --out docs src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/medpromptjs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/medpromptjs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@langchain/google-genai": "^0.2.9",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "dts-cli": "^2.0.3",
    "google-auth-library": "^8.9.0",
    "husky": "^8.0.3",
    "size-limit": "^11.0.1",
    "supertest": "^6.3.3",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@langchain/community": "0.3.44",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "langchain": "^0.3.27",
    "reflect-metadata": "^0.2.1",
    "tslog": "^4.9.2",
    "tsyringe": "^4.8.0"
  }
}
