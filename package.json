{
  "name": "medpromptjs",
  "version": "0.5.0",
  "description": "A library of langchain base classes for building medical applications.",
  "license": "Apache-2.0",
  "author": "Bell Eapen",
  "main": "dist/index.js",
  "module": "dist/medpromptjs.mjs",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/medpromptjs.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/medpromptjs.mjs": {
      "import": "./dist/medpromptjs.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build --target node",
    "postbuild": "mv dist/medpromptjs.esm.js dist/medpromptjs.mjs && mv dist/medpromptjs.esm.js.map dist/medpromptjs.mjs.map && sed -i 's/medpromptjs.esm.js.map/medpromptjs.mjs.map/g' dist/medpromptjs.mjs",
    "lint": "dts lint --fix",
    "prepare": "dts build --target node",
    "size": "size-limit",
    "start": "dts watch --target node",
    "test": "dts test --coverage --verbose --testPathIgnorePatterns=./test/e2e/*",
    "e2e": "dts test --verbose ./test/e2e --detectOpenHandles",
    "docs": "typedoc --exclude src/tool.ts --html docs src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/medpromptjs.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/medpromptjs.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/supertest": "^6.0.2",
    "dts-cli": "^2.0.5",
    "google-auth-library": "^10.1.0",
    "husky": "^9.1.7",
    "size-limit": "^12.0.0",
    "supertest": "^7.1.1",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.28.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@langchain/community": "^0.3",
    "@langchain/core": "^0.3",
    "cors": "^2.8",
    "dotenv": "^16",
    "express": "^5.1",
    "langchain": "^0.3",
    "reflect-metadata": "^0.2",
    "tslog": "^4.9",
    "tsyringe": "^4.10"
  }
}